<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <include layout="@layout/title_for_common" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <FrameLayout
            android:id="@+id/baijia_main_framelayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/baijia_main_foot_linearlayout"
            android:layout_marginBottom="1dp" />

        <LinearLayout
            android:id="@+id/baijia_main_foot_linearlayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:minHeight="50dp"
            android:orientation="horizontal"
            android:padding="@dimen/shop_main_width5_dimen" >
        </LinearLayout>
    </RelativeLayout>

</LinearLayout>